			"\t\t\t\t\t\t\t\tnew() {",
			"\t\t\t\t\t\t\t\t\t\t\t\t{ \"webSearchResults\", webSearchResults.Take(35) },",
			"\t\t\t\t\t\t\t\t\t\t\t\t{ \"topicOfResearch\", topicOfResearch }",
			"\t\t\t\t\t\t\t\t}",
			"\t\t\t\t\t\t);",
			"",
			"\t\t// Stop the timer",
			"\t\tsw.Stop();"
		]
	},
	"sk19": {
		"scope": "csharp",
		"prefix": "sk19",
		"body": [
			"\t\tConsole.WriteLine(\\$\"OUTCOME\t\\n\");",
			"\t\tConsole.WriteLine(\\$\"Milliseconds: {sw.ElapsedMilliseconds} \\n\");",
			"\t\tlong totalMilliseconds = sw.ElapsedMilliseconds;",
			"\t\tlong minutes = totalMilliseconds / (1000 * 60);",
			"\t\tlong seconds = (totalMilliseconds / 1000) % 60;",
			"\t\tlong milliseconds = totalMilliseconds % 1000;",
			"\t\tConsole.WriteLine(\\$\"{minutes} min, {seconds} sec, {milliseconds} ms\");",
			""
		]
	},
	"sk20": {
		"scope": "csharp",
		"prefix": "sk20",
		"body": [
			"\t\tstring resultantString = customHandlebarsPromptResult.GetValue<string>();",
			"\t\tvar correctedOutput = resultantString.Replace(\"\\\\n\", \"\\n\");",
			""
		]
	},
	"sk21": {
		"scope": "csharp",
		"prefix": "sk21",
		"body": [
			"\t\t// Store to file",
			"\t\tawait File.WriteAllTextAsync(searchResultsSummarizedFileName, correctedOutput);",
			"",
			"\t\tConsole.WriteLine(\\$\" Result: {correctedOutput}\");",
			"",
			"\t\treturn resultantString;"
		]
	}
}