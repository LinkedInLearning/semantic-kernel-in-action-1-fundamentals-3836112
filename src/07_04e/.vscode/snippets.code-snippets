			"\t\t\t\t\t\t{{set \"\"searchResults\"\" (concat\t\"\"---\\n\"\" \"\" Search Results \\n Topic: \"\" topicOfResearch \"\"\\n---\\n\"\")}}",
			"",
			"\t\t\t\t\t\t{{#each webSearchResults}}",
			"\t\t\t\t\t\t\t\t{{set \"\"summary\"\" (WebSearchAnalysis-Summarize this.Name this.Snippet )}}",
			"\t\t\t\t\t\t\t\t{{set \"\"searchResults\"\" (concat searchResults \"\"URL: \"\" this.Url\t\"\"\\n\"\")}}",
			"\t\t\t\t\t\t\t\t{{set \"\"searchResults\"\" (concat searchResults \"\"Name: \"\" this.Name\t\"\"\\n\"\")}}\t\t\t\t\t\t\t\t\t",
			"\t\t\t\t\t\t\t\t{{set \"\"score\"\" (WebSearchAnalysis-DetermineRelevance topicOfResearch this.Name this.Snippet )}}",
			"\t\t\t\t\t\t\t\t{{set \"\"searchResults\"\" (concat searchResults \"\"Relevance: \"\" score\t\"\"\\n\"\")}}\t\t\t\t\t\t\t ",
			"\t\t\t\t\t\t\t\t{{set \"\"searchResults\"\" (concat searchResults \"\"Summary: \"\" summary\t\"\"\\n\"\")}}",
			"\t\t\t\t\t\t\t\t{{set \"\"searchResults\"\" (concat searchResults \"\"---\\n\"\")}}\t\t\t\t\t\t\t\t\t",
			"\t\t\t\t\t\t{{/each}}",
			"",
			"\t\t\t\t\t\t{{set \"\"reportForResearchTopic\"\" (WebSearchAnalysis-GenerateResearchReport topicofresearch=topicOfResearch searchresults=searchResults)}}",
			"",
